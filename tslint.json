{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-bitwise": false,
    "no-conditional-assignment": false,
    "no-console": false,
    "no-namespace": false,
    "no-shadowed-variable": false,
    "object-literal-sort-keys": false,
    "prefer-const": true,
    "prefer-for-of": false,
    "quotemark": [ true, "double" ],
    "variable-name": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "libs/**/*",
      "node_modules/**/*",
      "scripts/**/*",
      "sub/**/*",
      "test/**/*"
    ]
  }
}
