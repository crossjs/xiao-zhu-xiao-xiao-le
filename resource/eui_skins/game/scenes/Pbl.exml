<?xml version='1.0' encoding='utf-8'?>
<e:Skin class="PblSkin"
 xmlns:e="http://ns.egret.com/eui" xmlns:w="http://ns.egret.com/wing">
	<e:Group id="bg" x="0" y="0" visible="true">
		<e:Image id="_mask" touchEnabled="true" fillMode="repeat" visible="true" source="sprites_json.mask_bg" left="0" right="0" top="0" bottom="0"/>
