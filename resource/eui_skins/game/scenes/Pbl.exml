				<e:Image source="sprites_json.jf" x="39" y="60"/>
			</e:Group>
			<e:Group x="465" y="306">
				<e:BitmapLabel id="tfdCoins" x="0" y="96" width="180" height="60" textAlign="center" font="score_fnt" scaleX="0.75" scaleY="0.75"/>
				<e:Image source="sprites_json.coin" x="44" y="0"/>
				<e:Image source="sprites_json.jb" x="39" y="60"/>
			</e:Group>
			<e:Group x="309" y="540">
				<e:Image source="sprites_json.zgfs" x="-20" y="0"/>
