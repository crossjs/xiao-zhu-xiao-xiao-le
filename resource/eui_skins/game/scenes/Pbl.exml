		<e:Group id="body" y="20" width="750" x="0">
			<e:Image id="btnBack" source="sprites_json.left" x="180" y="10"/>
			<e:Group x="300" y="306">
				<e:BitmapLabel id="tfdPoints" x="0" y="96" width="180" height="60" font="score_fnt" textAlign="center" scaleX="0.75" scaleY="0.75"/>
