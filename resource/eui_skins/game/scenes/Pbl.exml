			</e:Group>
			<e:Button id="btnHome" y="930.825" skinName="ButtonDefaultSkin" label="返回主页" x="195" width="240" enabled="true" height="90"/>
			<e:Button id="btnRestart" skinName="ButtonDefaultSkin" label="重新开始" y="930.825" x="465" width="240" enabled="true" height="90"/>
			<e:ToggleButton id="btnSound" y="150" x="387" enabled="true" selected="true" skinName="ButtonSoundSkin"/>
			<e:ToggleButton id="btnVibration" y="150" x="465" enabled="true" selected="true" skinName="ButtonVibrationSkin"/>
		</e:Group>
	</e:Group>
</e:Skin>
